<?xml version="1.0" encoding="UTF-8"?>
<jax-ws xmlns="http://www.netbeans.org/ns/jax-ws/1">
  <services>
    <service name="NewWebService">
      <implementation-class>com.MyPackage.Services.NewWebService</implementation-class>
    </service>
    <service name="OrderTrackingWebservice">
      <implementation-class>com.MyPackage.Services.OrderTrackingWebservice</implementation-class>
    </service>
  </services>
  <clients>
    <client name="NewWebService">
      <wsdl-url>http://localhost:8080/EnterpriseApplication1-war/NewWebService?wsdl</wsdl-url>
      <local-wsdl-file>localhost_8080/EnterpriseApplication1-war/NewWebService.wsdl</local-wsdl-file>
      <package-name forceReplace="true">com.MyPackage.WSDL</package-name>
      <catalog-file>catalog.xml</catalog-file>
      <wsimport-options>
        <wsimport-option>
          <wsimport-option-name>extension</wsimport-option-name>
          <wsimport-option-value>true</wsimport-option-value>
        </wsimport-option>
        <wsimport-option>
          <wsimport-option-name>verbose</wsimport-option-name>
          <wsimport-option-value>true</wsimport-option-value>
        </wsimport-option>
        <wsimport-option>
          <wsimport-option-name>fork</wsimport-option-name>
          <wsimport-option-value>false</wsimport-option-value>
        </wsimport-option>
        <wsimport-option>
          <wsimport-option-name>wsdlLocation</wsimport-option-name>
          <wsimport-option-value>http://localhost:8080/EnterpriseApplication1-war/NewWebService?wsdl</wsimport-option-value>
        </wsimport-option>
        <wsimport-option>
          <wsimport-option-name>xnocompile</wsimport-option-name>
          <wsimport-option-value>true</wsimport-option-value>
        </wsimport-option>
        <wsimport-option>
          <wsimport-option-name>xendorsed</wsimport-option-name>
          <wsimport-option-value>true</wsimport-option-value>
        </wsimport-option>
        <wsimport-option>
          <wsimport-option-name>package</wsimport-option-name>
          <wsimport-option-value>com.MyPackage.WSDL</wsimport-option-value>
        </wsimport-option>
      </wsimport-options>
    </client>
  </clients>
</jax-ws>
